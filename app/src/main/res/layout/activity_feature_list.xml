<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:background="#000000"
    tools:context="cop_4331c.gather.FeatureList">

    <!-- The primary full-screen view. This can be replaced with whatever view
         is needed to present your content, e.g. VideoView, SurfaceView,
         TextureView, etc. -->


    <!-- This FrameLayout insets its children based on system windows using
         android:fitsSystemWindows. -->
    <FrameLayout android:layout_width="match_parent" android:layout_height="match_parent"
        android:fitsSystemWindows="true">

        <LinearLayout android:id="@+id/fullscreen_content_controls"
            android:layout_width="match_parent" android:layout_height="wrap_content"
            android:layout_gravity="bottom|center_horizontal"
            android:background="@color/black_overlay" android:orientation="horizontal"
            tools:ignore="UselessParent">



        </LinearLayout>

        <ImageView
            android:layout_width="186dp"
            android:layout_height="116dp"
            android:layout_marginTop="@dimen/abc_action_bar_default_height_material"
            android:id="@+id/imageView"
            android:layout_gravity="left|top"
            android:background="@drawable/feature_name"
            android:alpha=".4"/>


        <ImageView
            android:layout_width="186dp"
            android:layout_height="116dp"
            android:layout_marginTop="@dimen/abc_action_bar_default_height_material"
            android:id="@+id/imageView2"
            android:layout_gravity="right|top"
            android:background="@drawable/feature_venue"
            android:alpha=".4"/>

        <ImageView
            android:layout_width="186dp"
            android:layout_height="116dp"
            android:id="@+id/ImageButton3"
            android:layout_gravity="left|center_vertical"
            android:background="@drawable/feature_invite"
            android:alpha=".4"/>

        <ImageButton
            android:layout_width="186dp"
            android:layout_height="116dp"
            android:id="@+id/ImageButton4"
            android:layout_gravity="right|center_vertical"
            android:background="@drawable/feature_music"
            android:alpha=".4"/>

        <ImageButton
            android:layout_width="186dp"
            android:layout_height="116dp"
            android:id="@+id/ImageButton5"
            android:layout_gravity="right|bottom"
            android:background="@drawable/feature_posting"
            android:layout_marginBottom="@dimen/abc_action_bar_default_height_material"
            android:alpha=".4"/>


        <TextView
            style="@style/Base.V21.Theme.AppCompat.Light.Dialog"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Let's get started..."
            android:textColor="#FFFFFF"
            android:id="@+id/textView"
            android:layout_gravity="top"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="5dp"/>

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="0dp"
            android:id="@+id/ImageButton14"
            android:layout_gravity="right|top"
            android:layout_marginRight="20dp"
            android:background="@drawable/emoji"
            android:alpha=".4"/>

        <TextView
            style="@style/Base.V21.Theme.AppCompat.Light.Dialog"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Name Event"
            android:textColor="#FFFFFF"
            android:id="@+id/textView2"
            android:layout_gravity="left"
            android:layout_marginTop="170dp"
            android:layout_marginLeft="5dp"/>

        <ImageButton
            style="@style/Base.V21.Theme.AppCompat.Light.Dialog"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Name Event"
            android:background="@drawable/feature_name_green"
            android:id="@+id/ImageButton6"
            android:layout_gravity="left"
            android:layout_marginTop="175dp"
            android:layout_marginLeft="160dp"/>

        <TextView
            style="@style/Base.V21.Theme.AppCompat.Light.Dialog"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Pick a Venue"
            android:textColor="#FFFFFF"
            android:id="@+id/textView4"
            android:layout_gravity="right"
            android:layout_marginTop="170dp"
            android:layout_marginRight="60dp"/>

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/feature_venue_green"
            android:id="@+id/ImageButton7"
            android:layout_gravity="right"
            android:layout_marginTop="175dp"
            android:layout_marginRight="10dp"/>

        <TextView
            style="@style/Base.V21.Theme.AppCompat.Light.Dialog"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Send Invites"
            android:textColor="#FFFFFF"
            android:id="@+id/textView5"
            android:layout_gravity="left"
            android:layout_marginTop="340dp"
            android:layout_marginLeft="5dp"/>

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/feature_invite_green"
            android:id="@+id/ImageButton8"
            android:onClick="PublishActivity"
            android:layout_gravity="left"
            android:layout_marginTop="345dp"
            android:layout_marginLeft="160dp"/>


        <TextView
            style="@style/Base.V21.Theme.AppCompat.Light.Dialog"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Create Playlist"
            android:textColor="#FFFFFF"
            android:id="@+id/textView7"
            android:layout_gravity="right"
            android:layout_marginTop="340dp"
            android:layout_marginRight="45dp"/>

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/feature_music_green"
            android:id="@+id/ImageButton9"
            android:layout_gravity="right"
            android:layout_marginTop="345dp"
            android:layout_marginRight="10dp"/>

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/feature_message_green"
            android:id="@+id/ImageButton13"
            android:layout_gravity="right"
            android:layout_marginTop="515dp"
            android:layout_marginRight="200dp"/>



        <TextView
            style="@style/Base.V21.Theme.AppCompat.Light.Dialog"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Post Photos"
            android:textColor="#FFFFFF"
            android:id="@+id/textView9"
            android:layout_gravity="right"
            android:layout_marginTop="510dp"
            android:layout_marginRight="65dp"/>

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/feature_photo_green"
            android:id="@+id/textView12"
            android:layout_gravity="right"
            android:layout_marginTop="515dp"
            android:layout_marginRight="15dp"/>

        <ImageButton
            android:layout_width="186dp"
            android:layout_height="116dp"
            android:id="@+id/ImageButton10"
            android:layout_gravity="left|bottom"
            android:layout_marginBottom="@dimen/abc_action_bar_default_height_material"
            android:background="@drawable/feature_dm"
            android:alpha=".4"/>

        <ImageButton
            android:layout_width="186dp"
            android:layout_height="116dp"
            android:id="@+id/ImageButton11"
            android:layout_gravity="left|bottom"
            android:layout_marginBottom="@dimen/abc_action_bar_default_height_material"
            android:background="@drawable/feature_dm"
            android:alpha=".4"/>

        <TextView
            style="@style/Base.V21.Theme.AppCompat.Light.Dialog"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Send Message"
            android:textColor="#FFFFFF"
            android:id="@+id/textView8"
            android:layout_gravity="left|bottom"
            android:layout_marginTop="510dp"
            android:layout_marginLeft="5dp"/>

    </FrameLayout>



</FrameLayout>
